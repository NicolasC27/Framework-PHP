{% extends 'layout.twig' %}

{% block css %}
    <link rel="stylesheet" href="/css/style.css" >
    {{ parent() }}
{% endblock %}

{% block body %}
    <nav class="iovox">
        <h1 id="logo">IOVOX</h1>
    </nav>
    <div class="calendar">
        <ul class="time-intervals">
            <li>9:00<span>AM</span></li>
            <li>9:30</li>
            <li>10:00<span>AM</span></li>
            <li>10:30</li>
            <li>11:00<span>AM</span></li>
            <li>11:30</li>
            <li>12:00<span>PM</span></li>
            <li>12:30</li>
            <li>1:00<span>PM</span></li>
            <li>1:30</li>
            <li>2:00<span>PM</span></li>
            <li>2:30</li>
            <li>3:00<span>PM</span></li>
            <li>3:30</li>
            <li>4:00<span>PM</span></li>
            <li>4:30</li>
            <li>5:00<span>PM</span></li>
            <li>5:30</li>
            <li>6:00<span>PM</span></li>
            <li>6:30</li>
            <li>7:00<span>PM</span></li>
            <li>7:30</li>
            <li>8:00<span>PM</span></li>
            <li>8:30</li>
            <li>9:00<span>PM</span></li>
        </ul>
        <div class="events-container">
            {% for datas in data %}
                <div class="event" style="top: 50.0347%; height: 4.16956%; left: 35%; width: 65%;">Meeting</div>
            {% endfor %}
        </div>
    </div>
{% endblock %}